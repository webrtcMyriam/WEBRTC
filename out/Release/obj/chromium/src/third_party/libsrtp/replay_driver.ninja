    -Wl,--icf=all -Wl,-O1 -Wl,--as-needed -Wl,--gc-sections $
    -Wl,--no-as-needed -lpthread -Wl,--as-needed
libs = 
build replay_driver: link $
    obj/chromium/src/third_party/libsrtp/srtp/test/replay_driver.replay_driver.o $
    obj/chromium/src/third_party/libsrtp/libsrtp.a $
    obj/chromium/src/third_party/boringssl/libboringssl.a $
    obj/chromium/src/third_party/boringssl/libboringssl_asm.a || $
